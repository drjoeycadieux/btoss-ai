---
import Alerts from "../components/alerts.astro";
---

<html lang="en">
    <head>
        <meta charset="utf-8" />
        <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
        <meta name="viewport" content="width=device-width" />
        <meta name="generator" content={Astro.generator} />
        <title>Tbosss AI | Release Notes</title>
    </head>
    <body>
        <Alerts />
        <div>
            <div class="navbar-main-top navbar">
                <ul>
                    <li><p class="title">Tboss AI</p></li>
                    <li><a href="/">Home</a></li>
                    <li><a href="/docs">Docs</a></li>
                    <li><a href="/learn-more">Learn More</a></li>
                </ul>
            </div>
        </div>

        <div>
            <div>
                <!-- code goes here -->
            </div>
        </div>

        <style>
            body {
                padding: 0;
                margin: 0;
                background-color: #161616;
            }

            .navbar-main-top {
                padding: 10px;
                background-color: rgba(0, 0, 0, 0.863);
            }

            .navbar-main-top ul {
                display: block;
            }

            .navbar-main-top ul li {
                display: inline;
                margin: 5px;
            }

            .navbar-main-top ul li a {
                color: white;
                font-family: sans-serif;
                font-size: 19px;
                text-decoration: none;
            }

            .navbar-main-top ul li a a:hover {
                color: lightgray;
                text-decoration: underline;
            }

            .title {
                color: white;
                display: inline;
                font-size: 26px;
                font-family: sans-serif;
                font-weight: bold;
            }
        </style>
    </body>
</html>
